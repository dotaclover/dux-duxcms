{
  "name": "duxweb/duxcms",
  "type": "project",
  "description": "Lightweight project development system",
  "keywords": [
    "duxphp",
    "slimphp"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "duxweb/dux-lite": "main-dev"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Console\\": "console/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "/Volumes/Web/duxlite",
      "options": {
        "symlink": true
      }
    }
  ],
  "license": "MIT",
  "config": {
    "allow-plugins": {
      "duxphp/lite": true
    }
  }
}
